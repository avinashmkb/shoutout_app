<h1>Listing businesses</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Login ID</th>
    <th>Need SMS alerts?</th>
    <th>Unicel Virtual Number</th>
    <th>Need mcoupon?</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @businesses.each do |business| %>
  <tr>
    <td><%= business.name %></td>
    <td><%= business.login_id %></td>
    <td><%= business.sms_alerts %></td>
    <td><%= business.unicel_vnumber %></td>
    <td><%= business.mcoupon %></td>
    <td><%= link_to 'Show', business %></td>
    <td><%= link_to 'Edit', edit_business_path(business) %></td>
    <td><%= link_to 'Destroy', business, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Business', new_business_path %>
