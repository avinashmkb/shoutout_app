<h1>Listing feedback_topics</h1>

<table>
  <tr>
    <th>Feedback</th>
    <th>Trendingtopic</th>
    <th>Branch</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @feedback_topics.each do |feedback_topic| %>
  <tr>
    <td><%= feedback_topic.feedback_id %></td>
    <td><%= feedback_topic.trendingtopic_id %></td>
    <td><%= feedback_topic.branch_id %></td>
    <td><%= link_to 'Show', feedback_topic %></td>
    <td><%= link_to 'Edit', edit_feedback_topic_path(feedback_topic) %></td>
    <td><%= link_to 'Destroy', feedback_topic, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

